--[[
	UI Name: Dirt Ui Library
	UI Link: https://v3rmillion.net/showthread.php?tid=1046594
]]--

addLibrary = loadstring(game:HttpGet("https://pastebin.com/raw/GCM3Zpzg",true))()
addTable = {}
addWindown = addLibrary:CreateWindow("Nice")
addWindown:Section("Section")
addWindown:Button("Button", function()
   print("Nice")
end)
addWindown:Toggle("Toggle", {location= addTable, flag= "Toggle"}, function()
   print(addTable["Toggle"])
end)
addWindown:Slider("Slider", {location= addTable, min= 1, max= 64, default= 32, precise= nil --[[ 0.00 instead of 0 ]], flag= "Slider"}, function()
   print(addTable["Slider"])
end)
addWindown:Dropdown("Dropdown", {location= addTable, flag= "Dropdown", search= true --[[AddsSearchBar]], list= {"1","2"} --[[Wont work when PlayerList = true]], PlayerList= true --[[ Turns the list into the players in the server ]]}, function()
   print(addTable["Dropdown"])
end)
addWindown:Bind("KeyBind", {location= addTable, flag= "KeyBind", default= Enum.KeyCode.B}, function() -- Automatically stops when the gui is removed
   print(addTable["KeyBind"])
end)
addWindown:Box("Box", {location= addTable, flag= "Box", type= "number" --[[ Only Numbers automatically on false ]], hold= "Numbers" --[[ PlaceHolderText ]]}, function()
   print(addTable["Box"])
end)
addWindown:Search(Color3.fromRGB(255,0,255) --[[nil = Yellow]]) -- Ez searcher for if you have a lot of things
addWindown:String({string= "String"})
